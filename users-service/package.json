    "jsonwebtoken": "8.2.0",
    "sequelize": "5.21.2",
    "serverless-domain-manager": "^2.6.0",
    "tedious": "2.3.1"
  },
  "devDependencies": {
    "typescript": "2.8.1",
    "@types/node": "9.6.0",
    "@types/aws-sdk": "2.7.0",
    "@types/aws-lambda": "0.0.34",
    "@types/sequelize": "4.27.7",
    "@types/tedious": "1.8.32",
    "@types/jsonwebtoken": "7.2.6",
    "serverless-offline": "3.18.0",
    "serverless-plugin-typescript": "1.1.5"
  }
